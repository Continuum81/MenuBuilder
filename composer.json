{
    "name": "toxic-lemurs/menu-builder",
    "description": "A simple menu builder for Laravel 5.1.*",
    "keywords": [
        "menu",
        "menu-builder"
    ],
    "homepage": "https://github.com/ToxicLemurs/MenuBuilder",
    "license": "MIT",
    "authors": [
        {
            "name": "Derik Nel and Don Broughton",
            "email": "derik.nel.8312@gmail.com",
            "homepage": "https://github.com/ToxicLemurs/MenuBuilder",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "classmap": [
            "src/models",
            "src/Facades",
            "src/Http/Controllers"
        ],
        "psr-4": {
            "ToxicLemurs\\MenuBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "src/models",
            "src/Facades",
            "src/Http/Controllers"
        ],
        "psr-4": {
            "ToxicLemurs\\MenuBuilder\\Test\\": "tests",
            "ToxicLemurs\\MenuBuilder\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0"
        }
    }
}
